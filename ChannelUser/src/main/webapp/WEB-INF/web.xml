<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

       
         
 <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.user.ChannelLoginServlet</servlet-class>
 
 </servlet>        
 <servlet-mapping>
      <servlet-name>login</servlet-name>
      <url-pattern>/log</url-pattern>
 </servlet-mapping>
         
 
 
 
 <servlet>
     <servlet-name>channelinsert</servlet-name>
     <servlet-class>com.user.ChannelInsertServlet</servlet-class>
 </servlet>
 <servlet-mapping>
      <servlet-name>channelinsert</servlet-name>
      <url-pattern>/insert</url-pattern>
 </servlet-mapping>


<servlet>
    <servlet-name>channelupdate</servlet-name>
    <servlet-class>com.user.ChannelUpdateServlet</servlet-class>
    
</servlet> 
 <servlet-mapping>
       <servlet-name>channelupdate</servlet-name>
       <url-pattern>/update</url-pattern>
 </servlet-mapping>
 
 
 
 
 <servlet>
     <servlet-name>channeldelete</servlet-name>
     <servlet-class>com.user.ChannelDeleteServlet</servlet-class>
 </servlet>
 <servlet-mapping>
          <servlet-name>channeldelete</servlet-name>
          <url-pattern>/delete</url-pattern>
 </servlet-mapping>
 </web-app>